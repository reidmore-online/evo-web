<const/options=[
    {
        value: 1,
        text: "Option 1",
    },
    {
        value: 2,
        text: "Option 2",
        selected: true,
    },
    {
        value: 3,
        text: "Option 3",
    },
]>
<evo-listbox-button name="formFieldName">
    <for|option| of=options>
        <@option value=option.value selected=option.selected>${option.text}</>
    </for>
</evo-listbox-button>
